<!-- cs image + text component -->
<section class="cs-two-across">
  <div class="cs-flex cs-justify-between cs-flex-row-rev page-width">
    <div class="cs-item cs-flex cs-justify-center cs-align-center">
      <div class="cs-callout cs-callout-right">
        <h2>{{COMPONENT.text-subheading}}</h2>
        <p>{{COMPONENT.text-text}}</p>
      </div>
    </div>
    <div class="cs-item"><img src="{{COMPONENT.image-url}}" alt="{{COMPONENT.image-alt}}"/></div>
  </div>
</section>
